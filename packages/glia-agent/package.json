{
  "name": "@backbay/glia-agent",
  "version": "0.1.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "Emotion, cognition, and audio systems for AI agents",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./emotion": {
      "types": "./dist/emotion.d.ts",
      "import": "./dist/emotion.js"
    },
    "./cognition": {
      "types": "./dist/cognition.d.ts",
      "import": "./dist/cognition.js"
    },
    "./audio": {
      "types": "./dist/audio.d.ts",
      "import": "./dist/audio.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "zod": "^3.23.0"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "react": "^19.1.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.2"
  },
  "license": "MIT"
}
