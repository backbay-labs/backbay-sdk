{
  "name": "@backbay/api-client",
  "version": "0.1.0-alpha.1",
  "description": "Typed API client for Backbay services",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@backbay/contract": "0.1.0-alpha.1",
    "@elysiajs/eden": "^1.0.30"
  },
  "peerDependencies": {
    "elysia": ">=1.0.0"
  },
  "peerDependenciesMeta": {
    "elysia": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/bun": "^1.2.16",
    "@types/node": "^24.3.0",
    "elysia": "^1.0.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^2.0.0"
  }
}
