{
  "name": "@backbay/witness",
  "version": "0.1.0",
  "description": "Browser-side verification of Backbay attestations",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./wasm": {
      "import": "./wasm/cyntra_trust_wasm.js",
      "types": "./wasm/cyntra_trust_wasm.d.ts"
    }
  },
  "files": [
    "dist",
    "wasm"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@solana/web3.js": "^1.91.0",
    "viem": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  },
  "keywords": [
    "backbay",
    "verification",
    "attestation",
    "wasm",
    "ed25519",
    "merkle",
    "witness"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/backbay/witness"
  }
}
