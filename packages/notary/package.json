{
  "name": "@backbay/notary",
  "version": "0.1.0",
  "description": "Web3 attestations, IPFS, and verification",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "bin": {
    "notary": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "bun run --hot src/cli.ts",
    "build": "bun build src/index.ts src/cli.ts --outdir dist --target node && bunx tsc --emitDeclarationOnly --declaration --outDir dist",
    "start": "bun run dist/cli.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "bunx tsc --noEmit"
  },
  "dependencies": {
    "@ethereum-attestation-service/eas-sdk": "^2.9.0",
    "@ensdomains/ensjs": "^4.0.0",
    "@hono/node-server": "^1.13.0",
    "@hono/zod-validator": "^0.4.0",
    "@lit-protocol/lit-node-client": "^6.0.0",
    "@lit-protocol/types": "^6.0.0",
    "@tableland/sdk": "^7.0.0",
    "@web3-storage/w3up-client": "^16.0.0",
    "ethers": "^6.13.0",
    "hono": "^4.6.0",
    "siwe": "^2.3.0",
    "starknet": "^9.2.1",
    "viem": "^2.21.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^22.0.0",
    "tsup": "^8.3.5",
    "typescript": "^5.9.3",
    "vitest": "^2.0.0"
  },
  "author": "Backbay <dev@backbay.industries>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/backbay/backbay-sdk.git",
    "directory": "packages/notary"
  },
  "bugs": {
    "url": "https://github.com/backbay/backbay-sdk/issues"
  },
  "homepage": "https://github.com/backbay/backbay-sdk/tree/main/packages/notary#readme"
}
