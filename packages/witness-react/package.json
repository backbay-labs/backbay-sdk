{
  "name": "@backbay/witness-react",
  "version": "0.1.0",
  "description": "React hooks and components for Backbay attestation verification",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@backbay/witness": "^0.1.0"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "19.2.8",
    "react": "^18.2.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^2.0.0"
  },
  "keywords": [
    "backbay",
    "witness",
    "verification",
    "react",
    "hooks"
  ],
  "author": "Backbay <dev@backbay.industries>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/backbay/backbay-sdk.git",
    "directory": "packages/witness-react"
  },
  "bugs": {
    "url": "https://github.com/backbay/backbay-sdk/issues"
  },
  "homepage": "https://github.com/backbay/backbay-sdk/tree/main/packages/witness-react#readme"
}
